{
    "manifest_version": 2,

    "name" : "Compiuter",
    "description" : "Compiuter",
    "version": "0.0.2",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["google.js"],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://www.gutenberg.org/files/*",
                "https://www.gutenberg.org/cache/*"
            ],
            "js": [
                "jquery.min.js",
                "qrcode.min.js",
                "password_field.js",
                "morphic_text.js"
            ],
            "run_at": "document_end"
        }
    ],

    "icons": {
        "16": "16px.png",
        "48": "48px.png",
        "128": "128px.png"
    },

    "permissions": [
        "activeTab",
        "tabs"
    ]
}